suite MONAN-OPER
   defstatus suspended

   edit ECF_HOME "/mnt/beegfs/monan/users/renato/issues/ecflow-oper/MONAN-WorkFlow-OPER"
   edit ECF_HOST "egeon-login.cptec.inpe.br"
   edit ECF_INCLUDE "/mnt/beegfs/monan/users/renato/issues/ecflow-oper/MONAN-WorkFlow-OPER/includes"
   edit ECF_TRIES "1"

   edit EXP "GFS"
   edit RES "1024002"
   edit FCSTH "240"
   
   
   

   family MONAN
      cron 13:00

      task pre
         label Info "MONAN pre-processing."
         label date "YYYYMMDDHH"
         label VERSION "Version of model."
         
      task model
         trigger /MONAN-OPER/MONAN/pre eq complete
         label Info "MONAN Model."
         label date "YYYYMMDDHH"
         label VERSION "Version of Model"
         
      task post
         trigger /MONAN-OPER/MONAN/model eq complete
         label Info "MONAN post-processing - Convert_MPAS"
         label date "YYYYMMDDHH"
         label VERSION "Version of Convert_MPAS"
   
   endfamily
   
endsuite
